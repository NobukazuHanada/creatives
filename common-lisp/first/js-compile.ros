#!/bin/sh
#|-*- mode:lisp -*-|#
#|
exec ros -Q -- $0 "$@"
|#

(ql:quickload :parenscript)

(defun start ()
  (with-open-file (*standard-output* "main.js" :direction :output
                                     :if-exists :supersede)
    (princ (parenscript:ps-compile-file "first.lisp"))))

(defun main (&rest argv)
  (declare (ignorable argv))
  (start))

